{
	"name": "vz-price-guide-v2",
	"version": "0.0.0",
	"private": true,
	"scripts": {
		"dev": "vite --port 5173 --strictPort",
		"build": "vite build",
		"preview": "vite preview",
		"lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs --fix --ignore-path .gitignore",
		"format": "prettier --write src/",
		"cy:open": "cypress open",
		"cy:run": "cypress run",
		"test:e2e": "node scripts/run-e2e.js",
		"emulators": "firebase emulators:start --only firestore,auth --project demo-vz-price-guide --import=./.emulator-data --export-on-exit",
		"test:e2e:emu": "cross-env VITE_FIREBASE_EMULATORS=1 node scripts/run-e2e.js",
		"test:e2e:emu:exec": "cross-env VITE_FIREBASE_EMULATORS=1 firebase emulators:exec --only firestore,auth \"node scripts/run-e2e.js\""
	},
	"dependencies": {
		"@heroicons/vue": "^2.2.0",
		"axios": "^1.9.0",
		"cheerio": "^1.0.0",
		"firebase": "^10.5.0",
		"firebase-admin": "^13.4.0",
		"vue": "^3.3.4",
		"vue-router": "^4.2.5",
		"vuefire": "^3.1.17"
	},
	"devDependencies": {
		"@rushstack/eslint-patch": "^1.3.3",
		"@tailwindcss/typography": "^0.5.10",
		"@vitejs/plugin-vue": "^4.4.0",
		"@vue/eslint-config-prettier": "^8.0.0",
		"autoprefixer": "^10.4.16",
		"cross-env": "^7.0.3",
		"cypress": "^14.5.4",
		"eslint": "^8.49.0",
		"eslint-plugin-vue": "^9.17.0",
		"firebase-tools": "^14.12.0",
		"postcss": "^8.4.31",
		"prettier": "^3.0.3",
		"sass": "^1.69.4",
		"start-server-and-test": "^2.0.13",
		"tailwindcss": "^3.3.3",
		"vite": "^4.4.11",
		"wait-on": "^8.0.4"
	}
}
